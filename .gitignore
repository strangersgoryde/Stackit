*.jks
*.keystore
key.properties
android/key.properties

# Proguard
proguard-rules.pro.bak

# ====================
# iOS SPECIFIC
# ====================

# CocoaPods (regenerated from Podfile)
ios/Pods/
ios/.symlinks/
ios/Podfile.lock

# Xcode build
ios/build/
*.ipa
*.dSYM.zip
*.dSYM

# Swift Package Manager
ios/.build/
ios/Package.resolved

# ====================
# MACOS SPECIFIC
# ====================
macos/Pods/
macos/.symlinks/
macos/Podfile.lock
macos/build/
macos/.build/
macos/Package.resolved

# ====================
# SECRETS & CREDENTIALS (Safety net - NEVER commit these)
# ====================
*.env
*.env.*
.env.local
.env.*.local
secrets/
credentials/
*.pem
*.p12
*.pfx
*.cer
*.crt
*.key

# Firebase (contains project-specific secrets)
google-services.json
GoogleService-Info.plist
firebase_app_id_file.json
firebase.json
.firebaserc

# ====================
# APK/AAB OUTPUT (Generated by GitHub Actions, not local)
# ====================
*.apk
*.aab
*.aar

# ====================
# TEST & DEBUG
# ====================
test/.test_coverage.dart
*.lcov

# ====================
# MISC
# ====================
*.patch
*.diff

# ====================
# KEYSTORE & SIGNING (CRITICAL - NEVER COMMIT)
# ====================

# Keystore files - signing keys for app releases
*.jks
*.keystore
barakah-release.jks
barakah-keystore-base64.txt

# Key properties - contains passwords and paths
key.properties
android/key.properties

# Generated signing outputs
*.pem
*.p12
*.pfx

# ====================
# GITHUB ACTIONS / CI-CD ARTIFACTS
# ====================

# Local workflow test artifacts
act/
.act/

# Build artifacts that shouldn't be committed
*.apk
*.aab
*.aar