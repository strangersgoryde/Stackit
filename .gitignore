# ====================
*.log
*.tmp
*.temp
*.swp
*.swo
*.swn
*.bak
*.class
*.pyc
*.pyo
*.orig
*.rej

# ====================
# VERSION CONTROL
# ====================
.svn/
.hg/
.bzr/

# ====================
# BUILD TOOLS & HISTORY
# ====================
.buildlog/
.history
migrate_working_dir/
.atom/

# ====================
# ANDROID SPECIFIC
# ====================

# Gradle
.gradle/
android/.gradle/
android/gradlew.lock

# Android build outputs
android/app/debug/
android/app/profile/
android/app/release/
android/captures/

